{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "env": {
            "value": "DEV"
        },
        "resourceGroup":{
            "value": "<<RG-Name>>"
        },
        "Tags": {
            "value": "<<Tag-Value"
        },
        "IsAppRunning": {
            "value": "Running"
        },
        "UseCase": {
            "value": "<<Use-Case-Value>>"
        },
        "numeric-suffix": {
            "value": "<<Function-App-No.>>"
        },
        "location": {
            "value": "westeurope"
        },
        "storageAccountName": {
            "value": "<<storage-accoutn-name>>"
        },
        "storageAccountRG": {
            "value": "<<storage-account-rg>>"
        },
        "newOrExisting": {
            "value": "storage_newOrExisting"
        },
        "hostingPlanName": {
            "value": "<<App-Service-Plan-Name>>"
        },
        "serverFarmResourceGroup": {
            "value": "<<App-Service-Plan-RG>>"
        },
        "ASE_ResourceGroup": {
            "value": "<<App-Service-Environment-RG>>"
        },
        "ASE_Name": {
            "value": "<<App-Service-Environment-Name>>"
        },
        "APPINSIGHTS_INSTRUMENTATIONKEY": {
            "value": "<<Application-Insights-Key>>"
        },
        "subscriptionId": {
            "value": "subscriptionId_value"
        },
        "alwaysOn": {
            "value": true
        },
        "use32BitWorkerProcess": {
            "value": true
        },
        "netFrameworkVersion": {
            "value": "v6.0"
        },
        "virtualNetworkResourceGroup": {
            "value": "<<Virtual-Network-RG>>"
        },
        "virtualNetworkName": {
            "value": "<<Virtual-Network-Name>>"
        },
        "subnetNames": {
            "value": [
                {
                    "subnetName": "<<Subnet-Name-01>>",
                    "action": "Allow",
                    "ignoreMissingVNetServiceEndpoint": true
                },
                {
                    "subnetName": "<<Subnet-Name-02>>",
                    "action": "Allow",
                    "ignoreMissingVNetServiceEndpoint": true
                }
            ]
        },
        "access_restriction": {
            "value": "true"
        },
        "ipSecurityRestrictions": {
            "value": [
            {
                    "ipAddress": "10.0.0.0/0",
                    "action": "Allow",
                    "tag": "Default",
                    "priority": 99,
                    "name": "<<rule-Name>>",
                    "description": "<<rule-description>>"
                }
            ]
        },
        "secretsPermissions": {
            "value": [
                "get",
                "list"
            ]
        },
        "certificatePermissions": {
            "value": [
                "get",
                "list"
            ]
        },
        "SQL_CONNECTION_VALUE": {
            "value": "<<built-in-connector>> Example:@Microsoft.KeyVault(SecretUri=https://<<Key-vault-name>>.vault.azure.net/secrets/sqlconnection-value)"         
        }
    }
}
